{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12",
                "requirements": ["flask"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}